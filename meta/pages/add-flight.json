                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "FinalDate",
                              "showTime": true,
                              "showTimeOkButton": false,
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "datepicker",
                              "value": "${flight.FinalDate}",
                              "valueType": "DateTime"
                            },
                            {
                              "component": "FinalDate",
                              "name": "FinalDateRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "FinalDate is required",
